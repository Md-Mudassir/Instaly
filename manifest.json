{
  "name": "Instaly",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Accepts followers request automatically",
  "icons": { "128": "icon_128.png" },
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "browser_action": {
    "default_title": "Instaly!",
    "default_popup": "index.html"
  },
  "permissions": ["https://*/*", "http://*/*", "tabs"]
}
